<h1>index page</h1>
<%= link_to 'create new account here', new_user_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Address</th>
    <th>Password</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.address %></td>
      <td><%= user.password %></td>
      <td><%= link_to 'show', user_path(user.id) %></td>
      <td><%= link_to "Edit", edit_user_path(user.id)%></td>
      <!-- <td><%#= link_to 'Show', user_path(user.id) %></td> -->
      <td><%= link_to 'Delete',user_path(user.id), method: :delete,
          data: { confirm: 'are you sure' } %></td>
    </tr>
  <% end %>
</table>
